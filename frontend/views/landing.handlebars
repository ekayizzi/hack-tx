<div class="outer-page-container">
  {{> navbar }}
  <div class="page-content">
    {{> landingheader}}
    <div class="page-body">
      <div class="interaction-area" style="flex: 1 1 0; flex-direction: column; justify-content: flex-start; align-items: center; gap: 24px; display: inline-flex">
        <div style="align-self: stretch; height: 202px; flex-direction: column; justify-content: flex-start; align-items: center; gap: 16px; display: flex">
          <div style="width: 152px; height: 118px; position: relative">
            <div style="width: 104px; height: 104px; left: 24px; top: 0px; position: absolute; background: rgba(186, 222, 251, 0.50); border-radius: 9999px"></div>
            <div style="width: 115px; height: 72px; left: 18px; top: 12px; position: absolute; box-shadow: 0px 6.6666669845581055px 6.6666669845581055px -3.3333334922790527px rgba(16, 24, 40, 0.04)">
              <div style="width: 115px; height: 72px; left: 0px; top: 0px; position: absolute; background: linear-gradient(355deg, #1A78C2 0%, #2196F3 100%); border-radius: 5px"></div>
              <div style="width: 20px; height: 15px; left: 10px; top: 14px; position: absolute; background: #BADEFB; border-radius: 1.67px"></div>
              <div style="width: 26.67px; height: 16.67px; left: 78.33px; top: 12px; position: absolute">
                <div style="width: 16.67px; height: 16.67px; left: 10px; top: 0px; position: absolute; background: #2196F3; border-radius: 9999px"></div>
                <div style="width: 16.67px; height: 16.67px; left: 0px; top: 0px; position: absolute; background: #BADEFB; border-radius: 9999px"></div>
              </div>
              <div style="width: 95px; height: 3.33px; left: 10px; top: 60px; position: absolute; background: #BADEFB"></div>
            </div>
            <div style="width: 10px; height: 10px; left: 16px; top: 0px; position: absolute; background: rgba(188, 222, 251, 0.25); border-radius: 9999px"></div>
            <div style="width: 14px; height: 14px; left: 11px; top: 102px; position: absolute; background: rgba(188, 222, 251, 0.25); border-radius: 9999px"></div>
            <div style="width: 14px; height: 14px; left: 138px; top: 28px; position: absolute; background: rgba(188, 222, 251, 0.25); border-radius: 9999px"></div>
            <div style="width: 8px; height: 8px; left: 130px; top: 4px; position: absolute; background: rgba(188, 222, 251, 0.25); border-radius: 9999px"></div>

            <div class="info-circle-gloss">
              <div class="info-circle-holder">
                <i class="fa-solid fa-circle-info fa-sm primary-color"></i>
              </div>
            </div>
          </div>
          <div style="align-self: stretch; height: 68px; flex-direction: column; justify-content: flex-start; align-items: center; gap: 4px; display: flex">
            <div style="align-self: stretch; text-align: center; color: #101828; font-size: 16px; font-family: Poppins; font-weight: 600; line-height: 24px; word-wrap: break-word">Start Your First Form</div>
            <div style="align-self: stretch; text-align: center; color: #475467; font-size: 14px; font-family: Inter; font-weight: 400; line-height: 20px; word-wrap: break-word">You have not created any forms in the system yet, please start by creating your first form now.</div>
          </div>
        </div>
        <div class="main-buttons-container">
          <button type="button" class="contact-us-button">
            <span>Contact Us</span>
          </button>
          <button type="button" class="new-form-button">
            <span>New Form</span>
            <i class="fa-solid fa-plus fa-sm" style="color: white"></i>
          </button>
        </div>
      </div>
      {{> landingCircle}}
    </div>
  </div>
</div>